{
  "name": "keycloakify",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "keycloakify sync-extensions",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-keycloak-theme": "pnpm run build && keycloakify build",
    "storybook": "storybook dev -p 3000",
    "build-storybook": "storybook build -o ./docs/storybook",
    "format": "prettier . --write",
    "clean": "rm -rf dist node_modules",
    "check:updates": "pnpm dlx pnpm-check-updates --deep --interactive --format group",
    "update": "pnpm dlx npm-check-updates -u && pnpm run refresh",
    "reinstall": "pnpm run clean && pnpm install",
    "rebuild": "pnpm run clean && pnpm install && pnpm run build-keycloak-theme",
    "prepush": "pnpm run lint && pnpm run test",
    "upgrade": "pnpm up -r",
    "refresh": "pnpm run clean && pnpm run upgrade && pnpm i && pnpm run dev"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsx-email": "^2.7.0",
    "keycloakify": "^11.8.15",
    "lucide-react": "^0.475.0",
    "react": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.20.0",
    "@keycloakify/email-native": "~260007.0.0",
    "@patternfly/patternfly": "^6.1.0",
    "@patternfly/react-core": "^6.1.0",
    "@patternfly/react-icons": "^6.1.0",
    "@patternfly/react-styles": "^6.1.0",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/addon-viewport": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/preset-create-react-app": "^8.5.8",
    "@storybook/react": "^8.5.8",
    "@storybook/react-vite": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@storybook/theming": "^8.5.8",
    "@tailwindcss/postcss": "^4.0.7",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^19.0.4",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^15.15.0",
    "keycloak-js": "26.1.2",
    "keycloakify-emails": "^3.1.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.5.8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  }
}
